#Configurar nome do usuario
git config --global user.name "felipek"	
confirmar: 
git config user.name

#Configurar nome do email
git config --global user.email ffkretzschmar@gmail.com
confirmar:
git config user.email


#Verificar Branch padrão
git config init.defaultBranch


#Trocar Branch
git config --global init.defaultBranch main


#Verificar configurações globais
git config --global --list
